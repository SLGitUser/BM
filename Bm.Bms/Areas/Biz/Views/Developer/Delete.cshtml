@model IList<Bm.Models.Dp.Developer>

@section Header{
    <script src="~/Content/JavaScript/Js_Tools.js"></script>
}

@section PageHead {
    <h1>
        开发商信息
        <small>（页面编号：0521）</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> 首页</a></li>
        <li class="active">开发商信息删除</li>
    </ol>
}
@using (Html.BeginForm())
{
        <!-- Main content -->
    <section class="content">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">确定删除这些数据吗？</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body table-responsive no-padding">
                    <table class="table table-hover">
                        <tbody>
                            <tr>
                                <th>编号</th>
                                <th>名称</th>
                            </tr>
                            @for (int i = 0; i < Model.Count; i++)
                            {
                                @Html.HiddenFor(m => m[i].Id)
                                <tr>
                                    <td>@Model[i].No</td>
                                    <td>@Model[i].Name</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        @*@Html.AntiForgeryToken()*@
        @*var longs = ViewBag.ids as List<long>;
            foreach (var item in longs)
            {
                var id = item;
                @Html.TextBox("ids",id)
            }*@
        <div class="form-actions no-color btn-group">
            <button type="submit" class="btn bg-blue btn-flat margin">
                <i class="fa fa-check"></i>
                确认删除
            </button>
            <a href="@Url.Action("Index")" class="btn bg-blue btn-flat margin">
                <i class="fa fa-mail-reply"></i>
                返回列表
            </a>
        </div>
    </section>
}