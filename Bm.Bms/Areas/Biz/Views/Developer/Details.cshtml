@model IList<Bm.Models.Dp.Developer>

@{

}
<script src="~/Content/JavaScript/Js_Tools.js"></script>
<section class="content-header">
    <h1>
        开发商信息
        <small>（页面编号：0521）</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> 首页</a></li>
        <li class="active">开发商信息查看</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">开发商信息查看</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body table-responsive no-padding">
                <table class="table table-hover">
                    <tbody>
                        <tr>
                            <th>编号</th>
                            <th>名称</th>
                        </tr>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.No</td>
                                <td>@item.Name</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
    </div>
    @*@Html.AntiForgeryToken()*@
    @Html.Hidden("ids",@ViewBag.ids as long[])
    <div class="form-actions no-color btn-group col-xs-12">
        <a href="@Url.Action("Index")" class="btn bg-blue btn-flat margin">
            <i class="fa fa-mail-reply"></i>
            返回列表
        </a>
    </div>
</section>